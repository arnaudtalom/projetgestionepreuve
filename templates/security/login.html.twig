<script src="front/assets/vendors/counter/waypoints-min.js"></script>
<script src="front/assets/vendors/counter/counterup.min.js"></script>
<script src="front/assets/vendors/imagesloaded/imagesloaded.js"></script>
<script src="front/assets/vendors/masonry/masonry.js"></script>
<script src="front/assets/vendors/masonry/filter.js"></script>
<script src="front/assets/vendors/owl-carousel/owl.carousel.js"></script>
<script src="front/assets/js/functions.js"></script>
<script src="front/assets/js/contact.js"></script>
<script src='front/assets/vendors/switcher/switcher.js'></script>
</body>

