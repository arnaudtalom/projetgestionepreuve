<body id="bg">
<div class="page-wraper">
	<div id="loading-icon-bx"></div>
	<div class="account-form">
		<div class="account-head" style="background-image:url(front/assets/images/background/medium-shot-people-graduating.jpg);">
			<a href="index.html"><img src="front/assets/images/logo-white-2.png" alt=""></a>
		</div>
		<div class="account-form-inner">
			<div class="account-container">
				<div class="heading-bx left">
					<h2 class="title-head">Connectez-vous à votre <span>Compte</span></h2>
				</div>	
				<form class="contact-bx" method="post">
                    {% if error %}
                        <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                    {% endif %}

                    {% if app.user %}
                        <div class="mb-3">
                            You are logged in as {{ app.user.username }}, <a href="{{ path('app_logout') }}">Logout</a>
                        </div>
                    {% endif %}

					<div class="row placeani">
						<div class="col-lg-12">
							<div class="form-group">
								<div class="input-group">
									<label>Votre Username</label>
									<input name="username" id="inputUsername" type="text" required="" class="form-control" value="{{ last_username }}">
								</div>
							</div>
						</div>
						<div class="col-lg-12">
							<div class="form-group">
								<div class="input-group"> 
									<label>Votre Password</label>
									<input type="password" name="password" id="inputPassword"  autocomplete="current-password"  class="form-control" required="">
								</div>
							</div>
                            <input type="hidden" name="_csrf_token"
                                value="{{ csrf_token('authenticate') }}"
                            >
						</div>
						<div class="col-lg-12">
							<div class="form-group form-forget">
								
								<a href="forget-password.html" class="ml-auto">Mot de passe oublié?</a>
							</div>
						</div>
						<div class="col-lg-12 m-b30">
							<button name="submit" type="submit" value="Submit" class="btn button-md">Me connecter</button>
						</div>

					</div>
				</form>
			</div>
		</div>
	</div>
</div>
<!-- External JavaScripts -->
<script src="front/assets/js/jquery.min.js"></script>
<script src="front/assets/vendors/bootstrap/js/popper.min.js"></script>
<script src="front/assets/vendors/bootstrap/js/bootstrap.min.js"></script>
